{% for flashMessage in app.flashes('notice') %}
    <div class="alert alert-success" role="alert">{{ flashMessage | raw }}</div>
{% endfor %}
{% for flashMessage in app.flashes('warning') %}
    <div class="alert alert-warning" role="alert">{{ flashMessage | raw }}</div>
{% endfor %}
{% for flashMessage in app.flashes('error') %}
    <div class="alert alert-danger" role="alert">{{ flashMessage | raw }}</div>
{% endfor %}
